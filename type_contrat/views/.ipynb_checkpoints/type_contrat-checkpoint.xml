<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <data>

        <record model="ir.ui.view" id="contrat_form_view">
            <field name="name">contrat</field>
            <field name="model">lb.contrat</field>
            <field name="arch" type="xml">
                <form string="contrat Form">
                    <sheet>
                      <group>
                                    <group>
                                        <field name="nom_contrat"/>
                                         <field name="seuil_min"/>
                                         <field name="status_contrat"/>
                                    </group>
                      </group>
                    </sheet>
                </form>
            </field>
        </record>

       

        <record model="ir.ui.view" id="contrat_tree_view">
            <field name="name">Contrat</field>
            <field name="model">lb.contrat</field>
            <field name="arch" type="xml">
                <tree string="contrat Tree">
                    <field name="nom_contrat"/>
                    <field name="seuil_min"/>
                    <field name="status_contrat"/>
                </tree>
            </field>
        </record>
        
       
         <record model="ir.actions.act_window" id="action_list_contrat">
            <field name="name">contrat</field>
            <field name="res_model">lb.contrat</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
       
         <menuitem id="menu_configuration_contrat" name="Type contrat" parent="immobilier.menu_configuration" sequence="12" action="action_list_contrat"/>
        
  </data>
</odoo>
       